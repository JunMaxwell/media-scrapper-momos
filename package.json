    "migrate:dev:pull": "cd backend && npm run migrate:dev:pull",
    "migrate:reset": "cd backend && npm run migrate:reset",
    "migrate:deploy:prod": "cd backend && npm run migrate:deploy:prod",
    "migrate:deploy:dev": "cd backend && npm run migrate:deploy:dev",
    "migrate:status": "cd backend && npm run migrate:status",
    "migrate:resolve": "cd backend && npm run migrate:resolve",
    "seed:dev": "cd backend && npm run seed:dev",
